/* pages/home/home.wxss */
page,
.contain {
    width: 750rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background-color: #1a8be9;
}

.logo {
    margin-top: -300rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 220rpx;
    height: 220rpx;
    border-radius: 110rpx;
    background-color: #fff;
}

.logo image {
    width: 220rpx;
    height: 220rpx;
}

.slogan {
    padding-top: 28px;
    font-size: 18px;
    color: #fff;
}

